//Document for definitions
{
    // To add functions:
    //attribute: 
    "functions":
    {
        "def":"Math.cos(x)",    //function respect x
        "color":"green"         //optional; default blue
    },

    //To add point
    //attribute: 
    "points":
    {
        "x": 0, 
        "y": 1, 
        "color": "pink"         //optional; default: red
    },

    //To add discrete functions
    //attribute
    "discretes":
    {
        "f": "n",       //function respect n
        //for linspace
        "N": 10,        //Number of samples
        "start": -4,    //starting point; default: 0
        //end point
        "end": 4,       //optional if "Fs" or "Ts" defined
        //for samplig period
        "Ts": null,     //optional if "end" or "Fs" defined
        //for sampling frequency
        "Fs": null,      //optional if "end" or "Ts" defined
        "color": "blue"  //optional; default: black
    }

}